{
  "name": "css-grid",
  "version": "1.0.1",
  "description": "CSS Grid playground",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "forever": "2.0.0",
    "livereload": "0.8.2",
    "ncp": "2.0.0",
    "npm-run-all": "4.1.5",
    "serve": "11.3.0"
  },
  "scripts": {
    "gh": "ncp index.html 404.html",
    "start": "npm-run-all -p serve livereload",
    "livereload": "livereload .",
    "serve": "serve .",
    "forever": "forever start ./node_modules/.bin/serve .",
    "logs": "forever -f logs 0",
    "stop": "forever stopall --killTree"
  },
  "keywords": [
    "css",
    "grid",
    "serve",
    "npm-run-all",
    "livereload"
  ],
  "author": "Maksim Kostromin aka daggerok",
  "license": "MIT"
}
